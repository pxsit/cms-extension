{
  "manifest_version": 3,
  "name": "CMS Extension",
  "version": "1.4",
  "description": "Extension for CMS grader and Show summary of A1, A2, A3 passed tasks and standard check for TOI preparation site.",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "css": ["style.css"],
      "matches": [
        "https://c2.thailandoi.org/*",
        "https://toi-coding.informatics.buu.ac.th/*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
